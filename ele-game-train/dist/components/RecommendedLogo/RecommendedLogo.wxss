.RecommendedLogo .video-mask {
  position: relative;
  width: 750rpx;
  height: 563rpx;
}
.RecommendedLogo .video-mask .mask {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.4);
}
.RecommendedLogo .video-mask .mask .play-btn {
  width: 100rpx;
  height: 100rpx;
